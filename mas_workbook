# Generate keygen on the running django server !!!! Important no passphrase for thread authentication
ssh-keygen -t ed25519 -f ~/.ssh/opnsense_key -C "opnsense-dns-parser"

# Copy keygen to the opnsense firewall
ssh-copy-id -i ~/.ssh/opnsense_key.pub root@192.168.5.1

# Test connection without key
ssh -i ~/.ssh/opnsense_key root@192.168.5.1 "echo OK"

